{
	"name": "kiwi",
	"description": "Bootstrap admin template",
	"main": "index.html",
	"authors": [
		"SpanTags"
	],
	"license": "ISC",
	"keywords": [
		"bootstrap",
		"admin",
		"template",
		"HTML5"
	],
	"homepage": "",
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"clean": {
		"bower_components": [
			"src/assets/vendor/bower_components"
		]
	},
	"copy": {
		"src": [
			"bower_components/jquery/dist/jquery.min.js",
			"bower_components/jquery-ui/jquery-ui.min.js",
			"bower_components/handlebars/handlebars.min.js",
			"bower_components/bootstrap/dist/js/bootstrap.min.js",
			"bower_components/bootstrap/scss/**",
			"bower_components/tether/dist/js/tether.min.js",
			"bower_components/breakpoints.js/dist/breakpoints.min.js",
			"bower_components/font-awesome/css/**",
			"bower_components/font-awesome/fonts/**",
			"bower_components/material-design-iconic-font/dist/**",
			"bower_components/css-hamburgers/_sass/**",
			"bower_components/switchery/dist/**",
			"bower_components/perfect-scrollbar/css/perfect-scrollbar.min.css",
			"bower_components/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js",
			"bower_components/jquery-slimscroll/jquery.slimscroll.min.js",
			"bower_components/waypoints/lib/jquery.waypoints.min.js",
			"bower_components/waypoints/lib/shortcuts/sticky.min.js",
			"bower_components/counterup/jquery.counterup.min.js",
			"bower_components/peity/jquery.peity.min.js",
			"bower_components/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js",
			"bower_components/moment/min/moment.min.js",
			"bower_components/fullcalendar/dist/fullcalendar.min.css",
			"bower_components/fullcalendar/dist/fullcalendar.min.js",
			"bower_components/bootstrap-daterangepicker/daterangepicker.css",
			"bower_components/bootstrap-daterangepicker/daterangepicker.js",
			"bower_components/lightbox2/dist/css/lightbox.min.css",
			"bower_components/lightbox2/dist/js/lightbox.min.js",
			"bower_components/lightbox2/dist/images/*",
			"bower_components/summernote/dist/summernote.css",
			"bower_components/summernote/dist/summernote.min.js",
			"bower_components/summernote/dist/font/*",
			"bower_components/datatables.net/js/jquery.dataTables.min.js",
			"bower_components/datatables.net-dt/css/jquery.dataTables.min.css",
			"bower_components/datatables.net-dt/images/*",
			"bower_components/jqvmap/dist/*.min.*",
			"bower_components/jqvmap/dist/maps/**/*",
			"bower_components/raphael/raphael.min.js",
			"bower_components/morris.js/morris.css",
			"bower_components/morris.js/morris.min.js",
			"bower_components/chart.js/dist/Chart.min.js",
			"bower_components/chartist/dist/scss/**/*",
			"bower_components/chartist/dist/chartist.min.css",
			"bower_components/chartist/dist/chartist.min.js",
			"bower_components/clockpicker/dist/**/*",
			"!bower_components/clockpicker/dist/**/*.min.*",
			"bower_components/sweetalert/dist/sweetalert.css",
			"bower_components/sweetalert/dist/sweetalert.min.js",
			"bower_components/jquery-validation/dist/*.min.js",
			"bower_components/twitter-bootstrap-wizard/jquery.bootstrap.wizard.min.js",
			"bower_components/parsleyjs/dist/parsley.min.js",
			"bower_components/parsleyjs/dist/i18n/**/*",
			"bower_components/animate.css/animate.min.css",
			"bower_components/jquery-circle-progress/dist/**"
		],
		"dest": "src/assets/vendor"
	},
	"dependencies": {
		"bootstrap": "v4.0.0-beta",
		"material-design-iconic-font": "^2.2.0",
		"css-hamburgers": "^0.5.0",
		"switchery": "^0.8.1",
		"jquery-slimscroll": "^1.3.8",
		"perfect-scrollbar": "^0.6.12",
		"font-awesome": "^4.7.0",
		"chartist": "gionkunz/chartist-js#^0.10.1",
		"jquery.easy-pie-chart": "^2.1.6",
		"counterup": "^1.0.0",
		"waypoints": "^4.0.1",
		"fullcalendar": "^3.0.1",
		"jquery-ui": "^1.12.1",
		"bootstrap-daterangepicker": "^2.1.24",
		"lightbox2": "^2.9.0",
		"breakpoints.js": "^1.0.4",
		"summernote": "^0.8.2",
		"jqvmap": "^1.5.1",
		"morris.js": "^0.5.1",
		"chart.js": "^2.5.0",
		"clockpicker": "^0.0.7",
		"sweetalert": "^1.1.3",
		"jquery-validation": "^1.16.0",
		"twitter-bootstrap-wizard": "^1.3.2",
		"parsleyjs": "^2.6.5",
		"animate.css": "^3.5.2",
		"jquery-circle-progress": "^1.2.2",
		"datatables.net": "^1.10.15",
		"datatables.net-dt": "^3.2.2"
	}
}
